{
  "name": "youtube-clip-downloader",
  "version": "2.0.0",
  "description": "Modern YouTube Clip Downloader with Web UI",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server.js",
    "client:dev": "echo Starting client... && node server.js",
    "start": "node server.js",
    "build": "echo Build complete",
    "setup": "node setup.js"
  },
  "keywords": [
    "youtube",
    "downloader",
    "clips",
    "yt-dlp",
    "web-app"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.0.0"
  }
}
